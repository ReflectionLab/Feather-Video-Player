  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "keywords": [
    "Feather",
    "vite",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ReflectionLab/Feather-Video-Player.git"
  },
  "homepage": "https://github.com/ReflectionLab/Feather-Video-Player",
  "bugs": "https://github.com/ReflectionLab/Feather-Video-Player/issues",
  "main": "./lib/feather-video-player.umd.js",
  "module": "./lib/feather-video-player.es.js",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/feather-video-player.es.js",
      "require": "./lib/feather-video-player.umd.js"
    }
  },
  "dependencies": {
    "element-plus": "^2.3.4",
    "flv.js": "^1.6.2",
    "hls.js": "^1.4.3",
    "v3hooks": "^1.5.0",
    "vue": "^3.3.2",
    "vue3-danmaku": "1.4.0-beta.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "unplugin-auto-import": "^0.16.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.9.0",
    "vue-tsc": "^1.6.4",
    "windicss": "^3.5.6"
